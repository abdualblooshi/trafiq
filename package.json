    "d3": "^7.9.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-ejs-layouts": "^2.5.1",
    "mapbox-gl": "^3.8.0",
    "path": "^0.12.7",
    "scrollama": "^3.2.0",
    "vega": "^5.30.0",
    "vega-embed": "^6.29.0",
    "vega-lite": "^5.21.0"
