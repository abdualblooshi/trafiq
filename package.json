{
  "name": "trafiq",
  "version": "1.0.0",
  "description": "ðŸš— TrafiQ is an interactive web application designed to visualize and analyze traffic incidents in Dubai. It features dynamic maps, real-time insights, and customizable accessibility settings to help users explore patterns and trends in traffic data effectively.",
  "main": "server/server.js",
  "scripts": {
    "setup": "node setup.js",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./public/css/styles.css",
    "watch:css": "tailwindcss -i ./src/css/input.css -o ./public/css/styles.css --watch",
    "build": "pnpm install --frozen-lockfile",
    "start": "node server/server.js",
    "dev": "nodemon server/server.js & pnpm run watch:css"
  },
  "keywords": [
    "traffic",
    "visualization",
    "dubai",
    "mapbox",
    "data-analysis",
    "accessibility"
  ],
  "author": "",
  "license": "ISC",
